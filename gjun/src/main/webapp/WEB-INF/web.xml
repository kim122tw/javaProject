<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>loginController</servlet-name>
  	<display-name>loginController</display-name>
  	<description></description>
  	<servlet-class>Controller.loginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addMemberController</servlet-name>
  	<display-name>addMemberController</display-name>
  	<description></description>
  	<servlet-class>Controller.addMemberController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addPorderController</servlet-name>
  	<display-name>addPorderController</display-name>
  	<description></description>
  	<servlet-class>Controller.addPorderController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteController</servlet-name>
  	<display-name>deleteController</display-name>
  	<description></description>
  	<servlet-class>Controller.deleteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deletePorderController</servlet-name>
  	<display-name>deletePorderController</display-name>
  	<description></description>
  	<servlet-class>Controller.deletePorderController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updatePorderController</servlet-name>
  	<display-name>updatePorderController</display-name>
  	<description></description>
  	<servlet-class>Controller.updatePorderController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginController</servlet-name>
  	<url-pattern>/loginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addMemberController</servlet-name>
  	<url-pattern>/addMemberController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addPorderController</servlet-name>
  	<url-pattern>/addPorderController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteController</servlet-name>
  	<url-pattern>/deleteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deletePorderController</servlet-name>
  	<url-pattern>/deletePorderController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updatePorderController</servlet-name>
  	<url-pattern>/updatePorderController</url-pattern>
  </servlet-mapping>
</web-app>
